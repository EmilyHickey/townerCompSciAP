package Barnyard;

/**
 *
 * @author EmilyHickey
 */
public class Bees extends Animal {

    private int numberOfWings;

    public Bees(String name, int wing) {
        super(name);
        this.numberOfWings = wing;
    }
    
   

    @Override
    public String toString() {
        return "Bees{" + "numberOfWings=" + numberOfWings + " food=" + this.getFood() + ", milesTraveled=" + this.getMilesTraveled() + ", name=" + this.getName() + '}';
    }

}
